{
  "name": "daheim-app-ui",
  "version": "0.1.0",
  "description": "Daheim App UI",
  "main": "lib/start.js",
  "scripts": {
    "start-dev": "../../node_modules/.bin/better-npm-run start-dev",
    "watch-client": "../../node_modules/.bin/better-npm-run watch-client",
    "dev": "../../node_modules/.bin/concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "check": "../../node_modules/.bin/eslint .",
    "build-babel": "../../node_modules/.bin/babel src/ -d lib/ --copy-files --source-maps true",
    "build-webpack": "NODE_ENV=production ../../node_modules/.bin/webpack --config webpack/production.config.js",
    "build": "npm run build-babel && npm run build-webpack",
    "clean": "rm -rf lib && rm -rf build",
    "start": "NODE_ENV=production node ./lib/start",
    "test": "../../node_modules/.bin/jest --no-cache",
    "cover": "../../node_modules/.bin/jest --no-cache --coverage && open tmp/coverage/lcov-report/index.html"
  },
  "bin": {
    "daheim-app-ui": "bin/daheim-app-ui.js"
  },
  "author": "Gergo Ertli",
  "license": "MIT",
  "betterScripts": {
    "start-dev": {
      "command": "babel-watch ./src/start",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "webpack-dev-server --config webpack/development.config.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egergo/daheim-app-ui.git"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "coverageDirectory": "<rootDir>/tmp/coverage",
    "testPathDirs": [
      "<rootDir>/src",
      "<rootDir>/__mocks__"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "core.js",
      "babel-polyfill",
      "babel-runtime",
      "crypto",
      "redux-actions",
      "flux-standard-action",
      "lodash",
      "sinon",
      "chai",
      "chai-as-promised"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.3.5",
    "bowser": "^1.4.2",
    "classnames": "^2.2.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "es6-error": "^3.0.1",
    "eslint-plugin-react": "^6.2.0",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "flux-standard-action": "^0.6.1",
    "howler": "^2.0.0",
    "http-proxy": "^1.13.2",
    "imports-loader": "^0.6.5",
    "lodash": "^4.11.2",
    "material-ui": "^0.15.0",
    "md5": "^2.1.0",
    "moment": "^2.12.0",
    "newrelic": "^1.28.2",
    "node-uuid": "^1.4.7",
    "raven": "^0.12.1",
    "raven-js": "^3.0.4",
    "react": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-dom": "^15.0.2",
    "react-dropzone": "^3.4.0",
    "react-helmet": "^3.0.1",
    "react-intl": "^2.1.3",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.11.0",
    "redux-thunk": "^2.0.1",
    "request-promise": "^4.1.1",
    "scroll-behavior": "^0.8.1",
    "serialize-javascript": "^1.2.0",
    "socket.io-client": "^1.4.6",
    "spdy": "^3.3.2",
    "webpack-isomorphic-tools": "^2.2.50",
    "webrtc-adapter": "^2.0.2",
    "whatwg-fetch": "^1.0.0"
  }
}
