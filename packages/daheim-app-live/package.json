{
  "name": "daheim-app-live",
  "version": "0.1.0",
  "description": "Socket.IO Server for Daheim",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node lib/app",
    "dev": "../../node_modules/.bin/better-npm-run dev",
    "check": "../../node_modules/.bin/eslint .",
    "build": "../../node_modules/.bin/babel src/ -d lib/ --copy-files",
    "clean": "rm -rf lib"
  },
  "betterScripts": {
    "dev": {
      "command": "../../node_modules/.bin/babel-watch src/app | ../../node_modules/.bin/bunyan -o short",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.0.5",
    "daheim-app-model": "*",
    "daheim-app-utils": "*",
    "debug": "^2.2.0",
    "express": "^4.13.1",
    "jsonwebtoken": "^5.4.1",
    "mongoose": "^4.4.3",
    "newrelic": "^1.22.0",
    "socket.io": "^1.4.6",
    "source-map-support": "^0.4.0",
    "spdy": "^3.3.2",
    "twilio": "^2.9.1"
  }
}
