{
  "name": "daheim-app-live",
  "version": "0.1.0",
  "description": "Socket.IO Server for Daheim",
  "main": "lib/app.js",
  "scripts": {
    "start": "NODE_ENV=production node lib/app",
    "dev": "better-npm-run dev",
    "check": "eslint .",
    "build": "../babel src/ -d lib/ --copy-files --source-maps true",
    "clean": "rm -rf lib"
  },
  "bin": {
    "daheim-app-live": "bin/daheim-app-live.js"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-watch src/app | bunyan -o short",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.0.5",
    "daheim-app-model": "*",
    "daheim-app-utils": "*",
    "debug": "^2.2.0",
    "express": "^4.13.1",
    "jsonwebtoken": "^7.1.9",
    "mixpanel": "^0.4.1",
    "moment": "^2.16.0",
    "mongoose": "^4.4.3",
    "newrelic": "^1.22.0",
    "node-uuid": "^1.4.7",
    "qs": "^6.2.1",
    "socket.io": "^1.4.6",
    "source-map-support": "^0.4.0",
    "spdy": "^3.3.2",
    "twilio": "^2.9.1",
    "web-push": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-watch": "^2.0.3",
    "better-npm-run": "0.0.13",
    "bunyan": "^1.8.5",
    "eslint": "^3.10.2"
  }
}
