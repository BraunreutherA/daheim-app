{
  "name": "daheim-app-api",
  "description": "API Server of Daheim",
  "version": "0.1.0",
  "license": "MIT",
  "main": "lib/app.js",
  "scripts": {
    "start": "NODE_ENV=production node lib/app",
    "dev": "../../node_modules/.bin/better-npm-run dev",
    "check": "../../node_modules/.bin/eslint .",
    "build": "../../node_modules/.bin/babel src/ -d lib/ --copy-files --source-maps true",
    "clean": "rm -rf lib"
  },
  "betterScripts": {
    "dev": {
      "command": "../../node_modules/.bin/babel-watch src/app | ../../node_modules/.bin/bunyan -o short",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "bin": {
    "daheim-app-api": "bin/daheim-app-api.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egergo/daheim-app-api.git"
  },
  "engines": {
    "node": "5.11.0",
    "npm": "3.8.8"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "azure-common": "^0.9.16",
    "azure-storage": "^1.3.0",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.0.5",
    "body-parser": "^1.13.3",
    "convict": "^1.0.1",
    "cookie-parser": "^1.4.1",
    "daheim-app-model": "*",
    "daheim-app-utils": "*",
    "debug": "^2.2.0",
    "express": "^4.13.1",
    "intl-format-cache": "^2.0.5",
    "intl-messageformat": "^1.3.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.5.8",
    "newrelic": "^1.22.0",
    "node-uuid": "^1.4.3",
    "passport": "^0.3.0",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0",
    "source-map-support": "^0.4.0",
    "spdy": "^3.3.2",
    "web-push": "^2.2.0"
  }
}
